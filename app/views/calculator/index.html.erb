      <button id="examples" class="newdropdown" data-dropdown="#savedpathway_choices">
        <i class="fa fa-list" aria-hidden="true"></i> Ejemplos
      </button>
      <button id="share_btn" onclick="window.share();">
        <i class="fa fa-clipboard" aria-hidden="true"></i> Compartir escenario
      </button>
    </div>
  </div>

