      <% end %>
    </div>

    <!-- Tabs -->
    <div id="views" class="tab">
      <button class="tablinks" onclick="window.openTab(event, 'energy')" id="defaultOpen">
        <i class="fa fa-bolt" aria-hidden="true"></i> Energía
      </button>
      <button class="tablinks" onclick="window.openTab(event, 'electricity')">
        <i class="fa fa-plug" aria-hidden="true"></i> Electricidad
      </button>
